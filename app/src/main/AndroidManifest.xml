<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.antonio.progetto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/WeFood"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <uses-permission android:name="android.permission.INTERNET" />
            </intent-filter>
        </activity>

        <activity android:name=".Registrazione">
            <intent-filter>
                <action android:name="com.example.antonio.progetto.Registrazione" />
                <category android:name="android.intent.category.DEFAULT" />
                </intent-filter>

        </activity>
        <activity android:name=".supermercato.MainSupermercato" />
        <activity android:name=".supermercato.ModificaInfo" />


    </application>

</manifest>